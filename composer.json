  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "symfony/dependency-injection": "^6.2",
    "symfony-bundles/json-request-bundle": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  }
