{
  "name": "ser/dto-request-bundle",
  "description": "Module for injecting DTO objects to response. Supports php 8 constructors and readonly properties.",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Rashin",
      "email": "ser@rashin.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ser\\DTORequestBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ser\\DTORequestBundle\\": "tests/"
    }
